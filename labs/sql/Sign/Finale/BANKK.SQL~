CREATE TABLE CUSTOMER11(
CUS_ID VARCHAR2(5) PRIMARY KEY,
NAME	VARCHAR2(10),
ADDRESS VARCHAR2(15));

CREATE TABLE ACCOUNT11(
NAME VARCHAR2(15),
ACNO NUMBER(7),
BALANCE NUMBER(9,2),
BR_ID VARCHAR2(5));

CREATE TABLE BRANCH11(
BR_ID VARCHAR2(5),
BR_CITY VARCHAR2(15));

CREATE TABLE CUSTOMER_TRIG11(
NAME VARCHAR2(15),
ADDRESS VARCHAR2(15));




INSERT INTO BRANCH11 VALUES('&BR_ID','&BR_CITY');

INSERT INTO ACCOUNT11 VALUES('&NAME',&ACNO,&BALANCE,'&BR_ID');

INSERT INTO CUSTOMER_TRIG11 VALUES('&NAME','&ADDRESS');

